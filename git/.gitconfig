[user]
	email = "contact@maxhu.dev"
	name = "maxhu08"
[credential]
	# use GitHub CLI for Git credentials; prefer Homebrew paths on macOS,
	# otherwise use /usr/bin/gh or gh from PATH.
	helper = "!case \"$(uname -s)\" in Darwin) gh_bin=/opt/homebrew/bin/gh; [ -x \"$gh_bin\" ] || gh_bin=/usr/local/bin/gh ;; *) gh_bin=/usr/bin/gh ;; esac; [ -x \"$gh_bin\" ] || gh_bin=gh; \"$gh_bin\" auth git-credential \"$@\""
